separator=true
separator_block_width=10

[caps-lock]
command=echo -e " caps $(xset -q | grep Caps | awk '{ print $4 }') "
interval=once
signal=10

[battery]
command=echo -e " bat $(acpi -b | grep -P -o '\d{1,3}%') "
interval=60

[time]
command=echo -e " $(date '+%A %d %B %Y, %I:%M %p') "
interval=60
