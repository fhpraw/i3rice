separator=false
separator_block_width=10

[network]
command=echo -e " INTERNET: $(nm-online -t 3 | grep -P -o 'online|offline') "
interval=60

[battery]
command=echo -e " BATTERY: $(acpi -b | grep -P -o '\d{1,3}%') "
interval=60

[time]
command=echo -e " $(date '+%R %A %d %B %Y') "
interval=60
color=#1d2021
background=#ebdbb2
